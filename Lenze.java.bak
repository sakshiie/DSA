class Lenze 
{
	public static void main(String[] args) 
	{
		int num=10000058900;
		int newNum=0;
		int rev=0;

		for(int i=num; i>0; i/=10)
		newNum=(i%10==0)?((newNum*10)+5):(newNum*10+(i%10));

		for(int i=newNum; i>0; i/=10)
		rev=rev*10+(i%10);
	    System.out.println(rev);
	}
}
