import java.util.*;

class LargestSum 
{
	public static void main(String[] args) 
	{
		int arr[]={1,2,3,4,5};
		int maxSum=Integer.MIN_VALUE;
		int maxPair[]=new int [2];

		for(int i=0; i<=arr.length; i++){
			for (int j=i+1; j<=arr.length; j++ )
			{
				int sum=arr[i]+arr[j];
				if(sum>maxSum){
					maxSum=sum;
			}

			maxPair[0]=arr[i];
			maxPair[1]=arr[j];

	}
}
System.out.println(maxSum);
	}
}
