public static int convertToInteger(String bin){
	int sum=0;
	for(int i=bin.length()-1, j=0; i>=0; i--, j++){
		sum+=((bin.charAt(i)*1-48)*findPower(2,j));
	}
	return sum;
}
public static int findPower(int num,int pow){
	int op=1;
	for(int i=1; i<=pow; i++)
		op*=num;
	return op;
}
public static String convertToBinary(int num){
	String bin="";
	for(int i=num; i>0; i/=2)
		bin=(i%2)+bin;
	return bin;
}
}